\name{runTophat}
\alias{runTophat}
\alias{runBowtie}
\title{
Execute command-line tool
}
\description{
Executes coommand-line tools based on definitions provided by \code{systemArgs}.
}
\usage{
runTophat(tophatargs, runid = "01")

runBowtie(bowtieargs, runid = "01")
}
%- maybe also 'usage' for other objects documented here.
\arguments{
  \item{tophatargs}{
	Uses as input object returned by call to \code{sytemsArgs}.
}
  \item{bowtieargs}{
	Uses as input object returned by call to \code{sytemsArgs}.
}
  \item{runid}{
	Run identifier used for log file to track system call commands. Default is \code{"01"}.
}
}
